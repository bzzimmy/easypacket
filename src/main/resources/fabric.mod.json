{
  "schemaVersion": 1,
  "id": "easypacket",
  "version": "${version}",
  "name": "EasyPacket",
  "description": "A Meteor Client addon for packet analysis and server reconnaissance.",
  "authors": [
    "bzzimmy"
  ],
  "contact": {
    "sources": "https://github.com/bzzimmy/easypacket"
  },
  "icon": "assets/easypacket/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.bzzimmy.easypacket.EasyPacket"
    ]
  },
  "mixins": [
    "easypacket.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.9"],
    "meteor-client": "*"
  }
}
